<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/activity_dining_hall_selection"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:paddingBottom="0dp"
    android:paddingLeft="0dp"
    android:paddingRight="0dp"
    android:paddingTop="0dp"
    tools:context="teammemes.tritonbudget.DiningHallSelection">

    <ScrollView
        android:id="@+id/DH_ScrollView_Container"
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        tools:ignore="UselessParent">

        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:orientation="vertical">

            <LinearLayout
                android:id="@+id/DH_LLH_DHBorder"
                android:layout_width="match_parent"
                android:layout_height="match_parent"
                android:background="@drawable/border_set_top"
                android:orientation="horizontal">

                <TextView
                    android:id="@+id/DH_TextView_DiningHall"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:gravity="center"
                    android:paddingBottom="8dp"
                    android:paddingEnd="8dp"
                    android:paddingStart="8dp"
                    android:paddingTop="8dp"
                    android:text="@string/dining_halls"
                    android:textAppearance="@style/TextAppearance.AppCompat.Body2"
                    android:textSize="16sp" />
            </LinearLayout>

            <LinearLayout
                android:id="@+id/DH_LLH_Border64"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:background="@drawable/border_set_top"
                android:orientation="horizontal">

                <TextView
                    android:id="@+id/DH_TextView_64"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:background="?android:selectableItemBackground"
                    android:clickable="true"
                    android:drawableStart="@mipmap/ic_launcher"
                    android:fontFamily="sans-serif"
                    android:lineSpacingExtra="4dp"
                    android:paddingBottom="8dp"
                    android:paddingEnd="8dp"
                    android:paddingStart="8dp"
                    android:paddingTop="8dp"
                    android:textAppearance="@style/TextAppearance.AppCompat" />
            </LinearLayout>

            <LinearLayout
                android:id="@+id/DH_LLH_BorderCafeV"
                android:layout_width="match_parent"
                android:layout_height="match_parent"
                android:background="@drawable/border_set_top"
                android:orientation="horizontal">

                <TextView
                    android:id="@+id/DH_TextView_CafeV"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:background="?android:attr/selectableItemBackground"
                    android:clickable="true"
                    android:drawableStart="@mipmap/ic_launcher"
                    android:lineSpacingExtra="4dp"
                    android:paddingBottom="8dp"
                    android:paddingEnd="8dp"
                    android:paddingStart="8dp"
                    android:paddingTop="8dp"
                    android:textAppearance="@style/TextAppearance.AppCompat" />
            </LinearLayout>

            <LinearLayout
                android:id="@+id/DH_LLH_BorderCanyonV"
                android:layout_width="match_parent"
                android:layout_height="match_parent"
                android:background="@drawable/border_set_top"
                android:orientation="horizontal">

                <TextView
                    android:id="@+id/DH_TextView_CanyonV"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:background="?android:attr/selectableItemBackground"
                    android:clickable="true"
                    android:drawableStart="@mipmap/ic_launcher"
                    android:lineSpacingExtra="4dp"
                    android:paddingBottom="8dp"
                    android:paddingEnd="8dp"
                    android:paddingStart="8dp"
                    android:paddingTop="8dp"
                    android:textAppearance="@style/TextAppearance.AppCompat" />
            </LinearLayout>

            <LinearLayout
                android:id="@+id/DH_LLH_BorderFoodworx"
                android:layout_width="match_parent"
                android:layout_height="match_parent"
                android:background="@drawable/border_set_top"
                android:orientation="horizontal">

                <TextView
                    android:id="@+id/DH_TextView_FoodWorx"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:background="?android:attr/selectableItemBackground"
                    android:clickable="true"
                    android:drawableStart="@mipmap/ic_launcher"
                    android:lineSpacingExtra="4dp"
                    android:paddingBottom="8dp"
                    android:paddingEnd="8dp"
                    android:paddingStart="8dp"
                    android:paddingTop="8dp"
                    android:textAppearance="@style/TextAppearance.AppCompat" />
            </LinearLayout>

            <LinearLayout
                android:id="@+id/DH_LLH_BorderOVT"
                android:layout_width="match_parent"
                android:layout_height="match_parent"
                android:background="@drawable/border_set_top"
                android:orientation="horizontal">

                <TextView
                    android:id="@+id/DH_TextView_OVT"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:background="?android:attr/selectableItemBackground"
                    android:clickable="true"
                    android:drawableStart="@mipmap/ic_launcher"
                    android:lineSpacingExtra="4dp"
                    android:paddingBottom="8dp"
                    android:paddingEnd="8dp"
                    android:paddingStart="8dp"
                    android:paddingTop="8dp"
                    android:textAppearance="@style/TextAppearance.AppCompat" />
            </LinearLayout>

            <LinearLayout
                android:id="@+id/DH_LLH_BorderPines"
                android:layout_width="match_parent"
                android:layout_height="match_parent"
                android:background="@drawable/border_set_top"
                android:orientation="horizontal">

                <TextView
                    android:id="@+id/DH_TextView_Pines"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:background="?android:attr/selectableItemBackground"
                    android:clickable="true"
                    android:drawableStart="@mipmap/ic_launcher"
                    android:lineSpacingExtra="4dp"
                    android:paddingBottom="8dp"
                    android:paddingEnd="8dp"
                    android:paddingStart="8dp"
                    android:paddingTop="8dp"
                    android:textAppearance="@style/TextAppearance.AppCompat" />
            </LinearLayout>


            <LinearLayout
                android:id="@+id/DH_LLH_FTBorder"
                android:layout_width="match_parent"
                android:layout_height="match_parent"
                android:background="@drawable/border_set_top"
                android:orientation="horizontal">

                <TextView
                    android:id="@+id/DH_Textview_FTSpeciality"
                    android:layout_width="0dp"
                    android:layout_height="match_parent"
                    android:layout_weight="1"
                    android:gravity="center"
                    android:paddingBottom="8dp"
                    android:paddingTop="8dp"
                    android:text="@string/food_trucks_and_speciality_restaurants"
                    android:textAppearance="@style/TextAppearance.AppCompat.Body2"
                    android:textSize="16sp" />
            </LinearLayout>

            <LinearLayout
                android:id="@+id/DH_LLH_BistroBorder"
                android:layout_width="match_parent"
                android:layout_height="match_parent"
                android:background="@drawable/border_set_top"
                android:orientation="horizontal">

                <TextView
                    android:id="@+id/DH_TextView_Bistro"
                    android:layout_width="0dp"
                    android:layout_height="wrap_content"
                    android:layout_weight="1"
                    android:background="?android:attr/selectableItemBackground"
                    android:clickable="true"
                    android:drawableStart="@mipmap/ic_launcher"
                    android:lineSpacingExtra="4dp"
                    android:paddingBottom="8dp"
                    android:paddingEnd="8dp"
                    android:paddingStart="8dp"
                    android:paddingTop="8dp"
                    android:textAppearance="@style/TextAppearance.AppCompat" />
            </LinearLayout>

            <LinearLayout
                android:id="@+id/DH_LLH_ClubMed"
                android:layout_width="match_parent"
                android:layout_height="match_parent"
                android:background="@drawable/border_set_top"
                android:orientation="horizontal">

                <TextView
                    android:id="@+id/DH_TextView_ClubMed"
                    android:layout_width="0dp"
                    android:layout_height="wrap_content"
                    android:layout_weight="1"
                    android:background="?android:attr/selectableItemBackground"
                    android:clickable="true"
                    android:drawableStart="@mipmap/ic_launcher"
                    android:lineSpacingExtra="4dp"
                    android:paddingBottom="8dp"
                    android:paddingEnd="8dp"
                    android:paddingStart="8dp"
                    android:paddingTop="8dp"
                    android:textAppearance="@style/TextAppearance.AppCompat" />
            </LinearLayout>

            <LinearLayout
                android:id="@+id/DH_LLH_FlavorFTBorder"
                android:layout_width="match_parent"
                android:layout_height="match_parent"
                android:background="@drawable/border_set_top"
                android:orientation="horizontal">

                <TextView
                    android:id="@+id/DH_TextView_FlavorFT"
                    android:layout_width="0dp"
                    android:layout_height="wrap_content"
                    android:layout_weight="1"
                    android:background="?android:attr/selectableItemBackground"
                    android:clickable="true"
                    android:drawableStart="@mipmap/ic_launcher"
                    android:lineSpacingExtra="4dp"
                    android:paddingBottom="8dp"
                    android:paddingEnd="8dp"
                    android:paddingStart="8dp"
                    android:paddingTop="8dp"
                    android:textAppearance="@style/TextAppearance.AppCompat" />
            </LinearLayout>

            <LinearLayout
                android:id="@+id/DH_LLH_GoodysBorder"
                android:layout_width="match_parent"
                android:layout_height="match_parent"
                android:background="@drawable/border_set_top"
                android:orientation="horizontal">

                <TextView
                    android:id="@+id/DH_TextView_Goodys"
                    android:layout_width="0dp"
                    android:layout_height="wrap_content"
                    android:layout_weight="1"
                    android:background="?android:attr/selectableItemBackground"
                    android:clickable="true"
                    android:drawableStart="@mipmap/ic_launcher"
                    android:lineSpacingExtra="4dp"
                    android:paddingBottom="8dp"
                    android:paddingEnd="8dp"
                    android:paddingStart="8dp"
                    android:paddingTop="8dp"
                    android:textAppearance="@style/TextAppearance.AppCompat" />
            </LinearLayout>

            <LinearLayout
                android:id="@+id/DH_LLH_GoodysFTBorder"
                android:layout_width="match_parent"
                android:layout_height="match_parent"
                android:background="@drawable/border_set_top"
                android:orientation="horizontal">

                <TextView
                    android:id="@+id/DH_TextView_GoodysFT"
                    android:layout_width="0dp"
                    android:layout_height="wrap_content"
                    android:layout_weight="1"
                    android:background="?android:attr/selectableItemBackground"
                    android:clickable="true"
                    android:drawableStart="@mipmap/ic_launcher"
                    android:lineSpacingExtra="4dp"
                    android:paddingBottom="8dp"
                    android:paddingEnd="8dp"
                    android:paddingStart="8dp"
                    android:paddingTop="8dp"
                    android:textAppearance="@style/TextAppearance.AppCompat" />
            </LinearLayout>

            <LinearLayout
                android:id="@+id/DH_LLH_RootsBorder"
                android:layout_width="match_parent"
                android:layout_height="match_parent"
                android:background="@drawable/border_set_top"
                android:orientation="horizontal">

                <TextView
                    android:id="@+id/DH_TextView_Roots"
                    android:layout_width="0dp"
                    android:layout_height="wrap_content"
                    android:layout_weight="1"
                    android:background="?android:attr/selectableItemBackground"
                    android:clickable="true"
                    android:drawableStart="@mipmap/ic_launcher"
                    android:lineSpacingExtra="4dp"
                    android:paddingBottom="8dp"
                    android:paddingEnd="8dp"
                    android:paddingStart="8dp"
                    android:paddingTop="8dp"
                    android:textAppearance="@style/TextAppearance.AppCompat" />
            </LinearLayout>

            <LinearLayout
                android:id="@+id/DH_LLH_64NBorder"
                android:layout_width="match_parent"
                android:layout_height="match_parent"
                android:background="@drawable/border_set_top_bottom"
                android:orientation="horizontal">

                <TextView
                    android:id="@+id/DH_TextView_64North"
                    android:layout_width="0dp"
                    android:layout_height="wrap_content"
                    android:layout_weight="1"
                    android:background="?android:attr/selectableItemBackground"
                    android:clickable="true"
                    android:drawableStart="@mipmap/ic_launcher"
                    android:lineSpacingExtra="4dp"
                    android:paddingBottom="8dp"
                    android:paddingEnd="8dp"
                    android:paddingStart="8dp"
                    android:paddingTop="8dp"
                    android:textAppearance="@style/TextAppearance.AppCompat" />
            </LinearLayout>
        </LinearLayout>
        <!--End of LinearLayout for Scrollview-->
    </ScrollView>

</RelativeLayout>